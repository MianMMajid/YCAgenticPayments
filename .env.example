# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/counter_db

# External API Keys
RENTCAST_API_KEY=your_rentcast_api_key_here
FEMA_API_KEY=your_fema_api_key_here
APIFY_API_TOKEN=your_apify_token_here
DOCUSIGN_INTEGRATION_KEY=your_docusign_integration_key_here
DOCUSIGN_SECRET_KEY=your_docusign_secret_key_here
DOCUSIGN_ACCOUNT_ID=your_docusign_account_id_here
DOCUSIGN_WEBHOOK_SECRET=your_docusign_webhook_secret_here

# Google Calendar API
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_CALENDAR_CLIENT_ID=your_google_client_id_here
GOOGLE_CALENDAR_CLIENT_SECRET=your_google_client_secret_here

# Redis Cache
REDIS_URL=redis://localhost:6379/0

# Encryption
ENCRYPTION_KEY=your_fernet_encryption_key_here

# OpenAI API (for property summaries)
OPENAI_API_KEY=your_openai_api_key_here

# Email Service (SendGrid or Gmail)
SENDGRID_API_KEY=your_sendgrid_api_key_here
GMAIL_API_CREDENTIALS=your_gmail_credentials_here

# Twilio SMS Service
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# CrimeoMeter API
CRIMEOMETER_API_KEY=your_crimeometer_api_key_here

# Agentic Stripe (Smart Contract Wallets)
AGENTIC_STRIPE_API_KEY=your_agentic_stripe_api_key_here
AGENTIC_STRIPE_WEBHOOK_SECRET=your_agentic_stripe_webhook_secret_here
AGENTIC_STRIPE_NETWORK=testnet

# Blockchain (Audit Trail)
BLOCKCHAIN_RPC_URL=your_blockchain_rpc_url_here
BLOCKCHAIN_NETWORK=testnet
BLOCKCHAIN_CONTRACT_ADDRESS=your_contract_address_here
BLOCKCHAIN_PRIVATE_KEY=your_private_key_here

# Application Settings
ENVIRONMENT=development
LOG_LEVEL=INFO

# Monitoring (Sentry)
SENTRY_DSN=your_sentry_dsn_here
SENTRY_TRACES_SAMPLE_RATE=0.1
SENTRY_PROFILES_SAMPLE_RATE=0.1
